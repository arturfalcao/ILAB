<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TLaboratorios" table="t_laboratorios">
    <unique-constraints>
      <unique-constraint name="IX_t_laboratorios_nrcontribuinte" columns="ft_nclaboratorio"/>
      <unique-constraint name="IX_t_laboratorios_nome" columns="ft_nome"/>
      <unique-constraint name="IX_t_laboratorios_codigo" columns="ft_codigo"/>
    </unique-constraints>
    <id name="fnId" type="bigint" column="fn_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ftNclaboratorio" type="string" column="ft_nclaboratorio" length="12" nullable="false"/>
    <field name="ftCodigo" type="string" column="ft_codigo" length="20" nullable="false"/>
    <field name="ftNome" type="string" column="ft_nome" length="100" nullable="false"/>
    <field name="ftAlias" type="string" column="ft_alias" length="100" nullable="true"/>
    <field name="ftMorada" type="string" column="ft_morada" length="100" nullable="false"/>
    <field name="ftCodpostal" type="string" column="ft_codpostal" length="10" nullable="false"/>
    <field name="ftLocalidade" type="string" column="ft_localidade" length="50" nullable="false"/>
    <field name="ftPais" type="string" column="ft_pais" length="20" nullable="false"/>
    <field name="ftTelefone" type="string" column="ft_telefone" length="20" nullable="false"/>
    <field name="ftFax" type="string" column="ft_fax" length="20" nullable="false"/>
    <field name="ftEmail" type="string" column="ft_email" length="100" nullable="false"/>
    <field name="ftObservacao" type="string" column="ft_observacao" length="300" nullable="true"/>
    <field name="ftNomecontacto" type="string" column="ft_nomecontacto" length="100" nullable="true"/>
    <field name="ftTelefonecontacto" type="string" column="ft_telefonecontacto" length="20" nullable="true"/>
    <field name="ftEmailcontacto" type="string" column="ft_emailcontacto" length="100" nullable="true"/>
  </entity>
</doctrine-mapping>
