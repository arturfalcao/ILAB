<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TLocaiscolheita" table="t_locaiscolheita">
    <unique-constraints>
      <unique-constraint name="IX_t_locaiscolheita_descricao" columns="ft_descricao"/>
      <unique-constraint name="IX_t_locaiscolheita_codigo" columns="ft_codigo"/>
    </unique-constraints>
    <id name="fnId" type="bigint" column="fn_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ftCodigo" type="string" column="ft_codigo" length="20" nullable="false"/>
    <field name="ftDescricao" type="string" column="ft_descricao" length="100" nullable="false"/>
    <field name="ftAlias" type="string" column="ft_alias" length="100" nullable="true"/>
    <field name="fnIdConcelho" type="bigint" column="fn_id_concelho" nullable="false"/>
    <field name="fnIdTipoLocalcolheita" type="bigint" column="fn_id_tipo_localcolheita" nullable="false"/>
    <field name="fnIdSistemaabastecimento" type="bigint" column="fn_id_sistemaabastecimento" nullable="false"/>
    <field name="fnIdCliente" type="bigint" column="fn_id_cliente" nullable="false"/>
    <field name="ftObservacao" type="string" column="ft_observacao" length="300" nullable="true"/>
    <field name="fbActivo" type="boolean" column="fb_activo" nullable="false"/>
  </entity>
</doctrine-mapping>
