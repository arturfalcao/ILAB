<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TAditivosfrascos" table="t_aditivosfrascos">
    <unique-constraints>
      <unique-constraint name="IX_t_aditivosfrascos_codigo" columns="fn_id"/>
    </unique-constraints>
    <id name="fnId" type="bigint" column="fn_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ftCodigo" type="string" column="ft_codigo" length="20" nullable="false"/>
    <field name="ftDescricao" type="string" column="ft_descricao" length="100" nullable="false"/>
    <field name="ftAlias" type="string" column="ft_alias" length="100" nullable="true"/>
    <field name="ftObservacao" type="string" column="ft_observacao" length="300" nullable="true"/>
    <field name="ftTipo" type="string" column="ft_tipo" length="100" nullable="true"/>
    <field name="ftFormulaquimica" type="string" column="ft_formulaquimica" length="100" nullable="true"/>
  </entity>
</doctrine-mapping>
