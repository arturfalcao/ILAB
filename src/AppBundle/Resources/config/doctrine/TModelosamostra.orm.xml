<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TModelosamostra" table="t_modelosamostra">
    <unique-constraints>
      <unique-constraint name="IX_t_modelosamostra_descricao" columns="ft_descricao"/>
    </unique-constraints>
    <id name="fnId" type="bigint" column="fn_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fbActivo" type="boolean" column="fb_activo" nullable="false"/>
    <field name="ftDescricao" type="string" column="ft_descricao" length="100" nullable="false"/>
    <field name="fnLimitedias" type="boolean" column="fn_limitedias" nullable="false"/>
    <field name="fnIdCliente" type="bigint" column="fn_id_cliente" nullable="false"/>
    <field name="fnIdTipoaprovacao" type="bigint" column="fn_id_tipoaprovacao" nullable="false"/>
    <field name="fnIdProduto" type="bigint" column="fn_id_produto" nullable="false"/>
    <field name="fnIdLegislacao" type="bigint" column="fn_id_legislacao" nullable="false"/>
    <field name="fnIdTipoamostra" type="bigint" column="fn_id_tipoamostra" nullable="false"/>
    <field name="fnIdTipocontrolo" type="bigint" column="fn_id_tipocontrolo" nullable="true"/>
    <field name="fnIdOrcamento" type="bigint" column="fn_id_orcamento" nullable="true"/>
    <field name="fnIdGrupoparametros" type="bigint" column="fn_id_grupoparametros" nullable="true"/>
    <field name="ftObservacao" type="string" column="ft_observacao" length="300" nullable="true"/>
  </entity>
</doctrine-mapping>
