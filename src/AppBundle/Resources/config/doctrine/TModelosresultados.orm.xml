<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TModelosresultados" table="t_modelosresultados">
    <unique-constraints>
      <unique-constraint name="IX_t_modelosresultados_descricao" columns="ft_descricao"/>
    </unique-constraints>
    <id name="fnId" type="bigint" column="fn_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fbActivo" type="boolean" column="fb_activo" nullable="false"/>
    <field name="fnIdUnidade" type="bigint" column="fn_id_unidade" nullable="false"/>
    <field name="fbIncluirnorelatorio" type="boolean" column="fb_incluirnorelatorio" nullable="false"/>
    <field name="fbGamavalores" type="boolean" column="fb_gamavalores" nullable="false"/>
    <field name="fnMaximo" type="decimal" column="fn_maximo" precision="10" scale="5" nullable="true"/>
    <field name="fnMinimo" type="decimal" column="fn_minimo" precision="10" scale="5" nullable="true"/>
    <field name="ftMensagemutilizador" type="string" column="ft_mensagemutilizador" length="200" nullable="true"/>
    <field name="fnIdTipoarredondamento" type="bigint" column="fn_id_tipoarredondamento" nullable="false"/>
    <field name="ftDescricao" type="string" column="ft_descricao" length="100" nullable="false"/>
    <field name="fnLimitequantificacao" type="decimal" column="fn_limitequantificacao" precision="10" scale="5" nullable="false"/>
    <field name="ftObservacao" type="string" column="ft_observacao" length="300" nullable="true"/>
  </entity>
</doctrine-mapping>
