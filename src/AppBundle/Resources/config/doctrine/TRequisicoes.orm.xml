<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TRequisicoes" table="t_requisicoes">
    <id name="fnId" type="bigint" column="fn_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fnIdLaboratorio" type="bigint" column="fn_id_laboratorio" nullable="true"/>
    <field name="fdUltimaemissao" type="datetime" column="fd_ultimaemissao" nullable="true"/>
    <field name="ftObservacao" type="string" column="ft_observacao" length="300" nullable="true"/>
    <field name="ftUltimoutilizador" type="string" column="ft_ultimoutilizador" length="100" nullable="true"/>
    <field name="fbAnulada" type="boolean" column="fb_anulada" nullable="true"/>
    <field name="fnDesconto" type="decimal" column="fn_desconto" precision="8" scale="2" nullable="true"/>
    <field name="ftTipopagamento" type="string" column="ft_tipopagamento" length="100" nullable="true"/>
    <field name="ftPrazoentrega" type="string" column="ft_prazoentrega" length="100" nullable="true"/>
  </entity>
</doctrine-mapping>
