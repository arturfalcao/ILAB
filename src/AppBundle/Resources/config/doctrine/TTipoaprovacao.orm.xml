<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TTipoaprovacao" table="t_tipoaprovacao">
    <id name="fnId" type="bigint" column="fn_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ftCodigo" type="string" column="ft_codigo" length="20" nullable="true"/>
    <field name="ftDescricao" type="string" column="ft_descricao" length="100" nullable="true"/>
  </entity>
</doctrine-mapping>
