<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TEntidadesgestoras" table="t_entidadesgestoras">
    <id name="fnId" type="bigint" column="fn_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ftCodigoirar" type="string" column="ft_codigoirar" length="50" nullable="false"/>
    <field name="ftDescricao" type="string" column="ft_descricao" length="100" nullable="false"/>
    <field name="ftAlias" type="string" column="ft_alias" length="100" nullable="true"/>
    <field name="ftMorada" type="string" column="ft_morada" length="100" nullable="false"/>
    <field name="ftCodpostal" type="string" column="ft_codpostal" length="10" nullable="false"/>
