<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TEspecificacoes" table="t_especificacoes">
    <unique-constraints>
      <unique-constraint name="IX_t_especificacoes_descricao" columns="ft_descricao"/>
      <unique-constraint name="IX_t_especificacoes_codigo" columns="ft_codigo"/>
    </unique-constraints>
    <id name="fnId" type="bigint" column="fn_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ftCodigo" type="string" column="ft_codigo" length="20" nullable="false"/>
    <field name="ftDescricao" type="string" column="ft_descricao" length="100" nullable="false"/>
    <field name="ftAlias" type="string" column="ft_alias" length="100" nullable="true"/>
    <field name="ftMensagemUtilizador" type="string" column="ft_mensagem_utilizador" length="300" nullable="true"/>
    <field name="ftTextoQdPassaP" type="string" column="ft_texto_qd_passa_p" length="300" nullable="true"/>
    <field name="ftTextoQdNaoPassaP" type="string" column="ft_texto_qd_nao_passa_p" length="300" nullable="true"/>
    <field name="ftTextoQdCumpreA" type="string" column="ft_texto_qd_cumpre_a" length="300" nullable="true"/>
    <field name="ftTextoQdNaoCumpreA" type="string" column="ft_texto_qd_nao_cumpre_a" length="300" nullable="true"/>
    <field name="fbEmissaoDeRelatorio" type="boolean" column="fb_emissao_de_relatorio" nullable="false"/>
    <field name="fnIdLegislacao" type="bigint" column="fn_id_legislacao" nullable="true"/>
    <field name="ftObservacao" type="string" column="ft_observacao" length="300" nullable="true"/>
    <field name="ftSiglavl" type="string" column="ft_siglavl" length="20" nullable="true"/>
    <field name="ftLegendavl" type="string" column="ft_legendavl" length="100" nullable="true"/>
    <field name="fbActivo" type="boolean" column="fb_activo" nullable="false"/>
  </entity>
</doctrine-mapping>
