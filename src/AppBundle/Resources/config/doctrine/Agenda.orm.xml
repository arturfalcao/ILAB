<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Agenda" table="agenda">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="text" column="title" nullable="true"/>
    <field name="descricao" type="text" column="Descricao" nullable="true"/>
    <field name="startdatetime" type="datetime" column="startDatetime" nullable="true"/>
    <field name="enddatetime" type="datetime" column="endDatetime" nullable="true"/>
    <field name="alldayevent" type="boolean" column="AllDayEvent" nullable="true"/>
    <field name="url" type="text" column="url" nullable="true"/>
    <field name="bgcolor" type="text" column="bgColor" nullable="true"/>
    <field name="fgcolor" type="text" column="fgColor" nullable="true"/>
    <field name="cssclass" type="text" column="cssClass" nullable="true"/>
  </entity>
</doctrine-mapping>
