<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TOrcamentos" table="t_orcamentos">
    <unique-constraints>
      <unique-constraint name="IX_t_orcamentos_codigo" columns="ft_codigo"/>
      <unique-constraint name="IX_t_orcamentos_descricao" columns="ft_descricao"/>
    </unique-constraints>
    <id name="fnId" type="bigint" column="fn_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ftCodigo" type="string" column="ft_codigo" length="20" nullable="false"/>
    <field name="ftDescricao" type="string" column="ft_descricao" length="100" nullable="false"/>
    <field name="ftAlias" type="string" column="ft_alias" length="100" nullable="true"/>
    <field name="fnIdCliente" type="bigint" column="fn_id_cliente" nullable="true"/>
    <field name="fdDatacriacao" type="datetime" column="fd_datacriacao" nullable="true"/>
    <field name="fdDataadjudicacao" type="datetime" column="fd_dataadjudicacao" nullable="true"/>
    <field name="ftRefexterna" type="string" column="ft_refexterna" length="20" nullable="true"/>
    <field name="ftObservacao" type="string" column="ft_observacao" length="300" nullable="true"/>
    <field name="fbActivo" type="boolean" column="fb_activo" nullable="false"/>
    <field name="fbValidado" type="boolean" column="fb_validado" nullable="false"/>
  </entity>
</doctrine-mapping>
